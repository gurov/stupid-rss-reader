{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html"
        ],
        "versionedFiles": [
          "/*.bundle.css",
          "/*.bundle.js",
          "/*.chunk.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "images",
      "urls": [
        "/**/*.png",
        "/**/*.jpg",
        "/**/*.jpeg",
        "/**/*.ico",
        "/**/*.bmp",
        "/**/*.gif",
        "/**/*.svg"
      ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "14d12h"
      }
    }
  ]
}
